<% split_form_for :calendar_event, @event, :url => admin_calendar_events_path, :html => {:multipart => true, :id => 'content_form'} do |f| -%>
	<%= render :partial => "form", :locals => { :f => f } %>
	
	<% f.buttons do %>
		<%= f.submit t(:"adva.common.save") %> <%= t(:'adva.common.connector.or') %> 
		<%= link_to t(:"adva.common.cancel"), admin_calendar_events_path %>
	<% end %>
<% end %>
