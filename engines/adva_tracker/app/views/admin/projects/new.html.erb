<% content_for :sidebar do %>
  <%= render "admin/shared/tracker_manage" %>
<% end %>

<h2><%= t(:'adva.tracker.label.new_project') %></h2>

<% split_form_for :project, @project, :url => admin_projects_path,
                  :html => {:multipart => true} do |f| %>

  <%= render "form", :f => f %>

  <% f.buttons do %>
    <%= submit_tag t(:"adva.common.save") %> <%= t(:"adva.common.connector.or") %>
    <%= link_to t(:"adva.common.cancel"), admin_trackers_path %>
  <% end %>
<% end %>
