<li id="<%= dom_id section %>" class="<%= section.type.downcase %> <%#= cycle '', 'highlight', :name => "sections" %>">
  <%= link_to object.title, admin_section_contents_path(object) %>
  <% unless object.children.empty? %>
  <ul>
    <%= render :partial => 'admin/shared/section', :collection => object.children %>
  </ul>
  <% end %>
</li>
