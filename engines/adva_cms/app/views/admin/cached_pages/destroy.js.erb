try {
  var count = $$('p.total strong').pop();
  if(count) {
    count.update(parseInt(count.innerHTML) - 1);
  }
  
  var row = $('<%= dom_id(@cached_page) %>');
  if(row) {
    row.addClassName('deleted');
  }
  
  Flash.notice("<%= t(:'adva.cached_pages.confirmation.cleared') %>")  

} catch(e) {
  alert(e)
}
