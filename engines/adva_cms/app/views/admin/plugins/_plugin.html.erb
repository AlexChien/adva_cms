<tr id="<%= dom_id plugin %>" class="<%= cycle("odd", "even") %>">
  <td>
		<%= view_resource_link(plugin, admin_plugin_path(@site, plugin), :text => plugin.name) %>
  </td>
  <td>
    <%= plugin.about['version'] %>
  </td>
  <td>
    <%= plugin.about['author'] %>
  </td>
  <td>
    <%= plugin.about['homepage'] %>
  </td>
</tr>
