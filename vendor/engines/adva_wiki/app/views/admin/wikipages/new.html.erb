<h1>Create: <%= @wikipage.title %></h1>
<% split_form_for :wikipage, @wikipage, :url => admin_wikipages_path, :html => {:multipart => true, :id => 'content_form'} do |f| -%>

	<%= render :partial => "form", :locals => {:f => f} %>
	
	<p class="buttons">
  		<%= f.submit "Apply Changes" %>
  		<%= link_to "cancel", admin_wikipages_path %>
	</p>
<% end %>
