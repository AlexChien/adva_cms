<h4>Profile</h4>

<fieldset>  
  <p>
    <%= f.label :first_name %>
    <%= f.text_field :first_name  %>
  </p>
  <p>
    <%= f.label :last_name %>
    <%= f.text_field :last_name  %>
  </p>
  <p>
    <%= f.label :homepage %>
    <%= f.text_field :homepage  %>
  </p> 
  <p>
    <%= f.label :email %>
    <%= f.text_field :email  %>
  </p> 
  <p>
    <%= f.label :password %>
    <%= f.password_field :password  %>
  </p> 
</fieldset>
